#include<bits/stdc++.h>
#include <iostream>

using namespace std;

int main()
{
    
   int n;
   cin>>n;
   
   while(n--)
   {
       int x,y,z;
       cin>>x>>y>>z;
       int a=x-z;
       if(a<0)
       {
           a=a*(-1);
       }
      
       int b=y-z;
        if(b<0)
       {
           b=b*(-1);
       }
       
       if(b<a)
       {
           cout<<"Cat B"<<endl;
       }
       else if(b>a)
       {
           cout<<"Cat A"<<endl;
       }
       else
       {
           cout<<"Mouse C"<<endl;
       }
       
   }
}
